[configuration]

entry_symbol = "gonzago_library_init"
compatibility_minimum = "4.1"

[libraries]

windows.release.x86_64 = "lib/windows/gonzago_windows-x86_64.dll"
windows.debug.x86_64 = "lib/windows/gonzago_windows_editor-x86_64.dll"
windows.release.x86_32 = "lib/windows/gonzago_windows-x86_32.dll"
windows.debug.x86_32 = "lib/windows/gonzago_windows_editor-x86_32.dll"

linux.release.x86_64 = "lib/linux/gonzago_linux-x86_64.so"
linux.debug.x86_64 = "lib/linux/gonzago_linux_editor-x86_64.so"
linux.release.arm64 = "lib/linux/gonzago_linux-arm64.so"
linux.debug.arm64 = "lib/linux/gonzago_linux_editor-arm64.so"
linux.release.rv64 = "lib/linux/gonzago_linux-rv64.so"
linux.debug.rv64 = "lib/linux/gonzago_linux_editor-rv64.so"

macos.release = "lib/macos/gonzago_macos.framework"
macos.debug = "lib/macos/gonzago_macos_editor.framework"

ios.release = "lib/ios/gonzago_ios.framework"
ios.debug = "lib/ios/gonzago_ios_editor.framework"

android.release.x86_64 = "lib/android/gonzago_android-x86_64.so"
android.debug.x86_64 = "lib/android/gonzago_android_editor-x86_64.so"
android.release.arm64 = "lib/android/gonzago_android-arm64.so"
android.debug.arm64 = "lib/android/gonzago_android_editor-arm64.so"